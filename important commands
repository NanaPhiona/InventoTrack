//creating laravel project
composer create-project laravel/laravel InventoTracker

cd project_name

running the project
php artisan serve

//creating models
php artisan make:model Company -m

//creating control
php artisan admin:controller --model=App\User

//updating a table
php artisan make:migration add_email_to_users_table --table=admin_users

//updating a table
php artisan make:migration add_in_stock_stock_sub_cgoriesategories --table=stock_sub_categories
